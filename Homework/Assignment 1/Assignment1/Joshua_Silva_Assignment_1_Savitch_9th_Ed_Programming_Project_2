#include <iostream>
using namespace std;

int main()
{
    cout <<"*******************************************************************\n";
    cout <<"        C C C                    S S S S         !!\n";
    cout <<"      C       C                S       S         !!\n";
    cout <<"     C                        S                  !!\n";
    cout <<"    C                          S                 !!\n";
    cout <<"    C                           S S S S          !!\n";
    cout <<"    C                                   S        !!\n";
    cout <<"     C                                   S       !!\n";
    cout <<"      C      C                  S       S\n";
    cout <<"        C C C                     S S S S        00\n";
    cout <<"*******************************************************************\n";
    cout <<"     Computer Science is Cool Stuff!!!";
   
    return 0;
}

